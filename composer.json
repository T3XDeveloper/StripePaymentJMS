{
    "name": "t3xdeveloper/stripe-payment-jms",
    "type": "symfony-bundle",
    "description": "Stripe Payment Provider Package based and developed on JMSPaymentCoreBundle. Allow access to Stripe API by symfony framework.",
    "keywords": ["payment", "stripe", "symfony", "ibexa", "ecommerce"],
    "homepage": "https://github.com/T3XDeveloper/StripePaymentJMS",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Wiermann",
            "email": "patrick@t3x-developer.de"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^5.4",
        "symfony/browser-kit": ">=2.3 <6.0",
        "symfony/config": ">=2.3 <5.0",
        "symfony/dependency-injection": ">=2.3 <6.0",
        "symfony/form": ">=2.3 <6.0",
        "symfony/http-foundation": ">=2.3 <6.0",
        "symfony/http-kernel": ">=2.3 <6.0",
        "jms/payment-core-bundle": "~1.0",
        "omnipay/stripe": "~3.0",
        "psr/log": "^1|^2|^3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.4",
        "symfony/phpunit-bridge": "~2.7"
    },
	"autoload": {
        "psr-0": {
            "JMS\\Payment\\StripeBundle": ""
        }
    },
    "target-dir": "JMS/Payment/StripeBundle"
}